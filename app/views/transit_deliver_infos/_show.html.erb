<div id="table_wrap"></div>
<table class='table'>
  <tbody>
    <tr>
      <td>中转部门:</td>
      <td><%=resource.org.name %></td>
      <td>提货日期:</td>
      <td><%=resource.bill_date.strftime('%Y-%m-%d')%></td>
      <td>中转手续费:</td>
      <td><%=resource.transit_hand_fee%></td>
      <td>运单号</td>
      <td><%=resource.carrying_bill%></td>
    </tr>
    <tr>
      <td>备注:</td>
      <td colspan="8"><%=resource.note%></td>
    </tr>
  </tbody>
</table>
