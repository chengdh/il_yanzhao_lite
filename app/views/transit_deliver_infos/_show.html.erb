<table class='table' data-detailUrl='<%=transit_deliver_info_carrying_bills_path(resource,:show_fields => "transit_hand_fee,.send_fee,.transit_fee,.agent_carrying_fee,.transit_bill_no,#{can?(:read_commission,CarryingBill)? '.transit_carrying_fee,.commission' : '.total_transit_carrying_fee'}",:hide_fields => ".insured_fee,.customer,.bill_no,.note")%>'>
  <tbody>
    <tr>
      <td>部门:</td>
      <td><%=resource.org.name %></td>
      <td>提货日期:</td>
      <td><%=resource.bill_date.strftime('%Y-%m-%d')%></td>
    </tr>
    <tr>
      <td>备注:</td>
      <td colspan="3"><%=resource.note%></td>
    </tr>
  </tbody>
</table>
<div id="table_wrap"></div>
