<table class='table'>
  <tr>
    <td>清单日期:</td>
    <td><%=resource.bill_date.strftime('%Y-%m-%d')%></td>
    <td>组织机构:</td>
    <td><%=resource.org.name%></td>
    <td>银行:</td>
    <td><%=resource.bank.try(:name)%></td>
    <td>操作员:</td>
    <td><%=resource.user.try(:username)%></td>
    <td>状态:</td>
    <td><%=resource.human_state_name%></td>
  </tr>
  <tr>
    <td>备注:</td>
    <td colspan='10'>
      <%= resource.note %>
    </td>
  </tr>
</table>
<div id="table_wrap"></div>
<script>
  //获取清单明细数据
  jQuery(function($) {$.get('<%=transfer_payment_list_carrying_bills_path(resource)%>',null,null,'script');});
</script>
