<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", load_lists_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_load_list_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> LoadLists</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.load_list.bill_date", :default => t("activerecord.labels.bill_date", :default => "Bill_date")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @load_lists.each do |load_list| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= load_list.id %>
          </td>
                    <td>
            <%= link_to load_list.bill_date, load_list_path(load_list) %>
          </td>
                    <td>
            <%= load_list.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", load_list_path(load_list) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_load_list_path(load_list) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", load_list_path(load_list), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
